{
    "name": "hpatoio/bitly-api",
    "type": "library",
    "description": "PHP Library based on Guzzle to consume Bit.ly API | Not all API method are mapped yes",
    "keywords": ["Bitly", "api", "guzzle"],
    "homepage": "http://www.iliveinperego.com/",
    "license": "CC-BY-SA-3.0",
    "authors": [
        {
            "name": "Simone Fumagalli",
            "email": "simone@iliveinperego.com"
        }
    ],
    "require": {
        "guzzle/guzzle": "3.7.*",
        "monolog/monolog": "1.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "behat/behat": "2.4.*@stable",
        "behat/mink": "1.4@stable"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-0": {
            "Bitly\\Api\\": "src/"
        }
    }
}
